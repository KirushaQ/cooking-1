						//=>nrel_form: some_form;
						//=>nrel_additional_component: some_additional_component;
						=>nrel_sequencing: test_dish_recept_stage2;;

test_dish_recept_stage2 <- test_dish_recept_stages;
						<- concept_stage;
						<=nrel_equipment:...
						(*
							->some_equipment2;;
						*);
						<=nrel_stage_products:...
						(*
							->some_products2;;
						*);
						=>nrel_processing_method: some_method2;
						=>nrel_final_product: some_final_product2;
						//=>nrel_form: some_form2;
						//=>nrel_additional_component: some_additional_component2;
